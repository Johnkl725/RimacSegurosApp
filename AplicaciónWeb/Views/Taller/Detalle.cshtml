@model EntidadesProyecto.Taller

<h2>Detalle del Taller</h2>

<div>
    <h4>@Model.Nombre</h4>
    <dl class="row">
        <dt class="col-sm-2">Dirección</dt>
        <dd class="col-sm-10">@Model.Direccion</dd>

        <dt class="col-sm-2">Teléfono</dt>
        <dd class="col-sm-10">@Model.Telefono</dd>

        <dt class="col-sm-2">Correo</dt>
        <dd class="col-sm-10">@Model.Correo</dd>

        <dt class="col-sm-2">Ciudad</dt>
        <dd class="col-sm-10">@Model.Ciudad</dd>

        <dt class="col-sm-2">Tipo</dt>
        <dd class="col-sm-10">@Model.Tipo</dd>

        <dt class="col-sm-2">Capacidad</dt>
        <dd class="col-sm-10">@Model.Capacidad</dd>

        <dt class="col-sm-2">Descripción</dt>
        <dd class="col-sm-10">@Model.Descripcion</dd>

        <dt class="col-sm-2">Calificación</dt>
        <dd class="col-sm-10">@Model.Calificacion</dd>

        <dt class="col-sm-2">Estado</dt>
        <dd class="col-sm-10">@Model.Estado</dd>
    </dl>
    <a asp-action="Index" class="btn btn-secondary">Volver a la lista</a>
</div>
